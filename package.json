{
    "name": "server",
    "version": "1.0.0",
    "description": "E-com app server",
    "main": "server.js",
    "engines": {
        "node": "12.13.0"
    },
    "scripts": {
        "server":"nodemon server.js",
        "client":"cd client && yarn start",
